{
  JsonNode node=jp.getCodec().readTree(jp);
  String rngClazz=node.textValue();
  int seed=node.asInt();
  try {
    Class<? extends Random> clazz=(Class<? extends Random>)Class.forName(rngClazz);
    Random gen=clazz.newInstance();
    gen.setSeed(seed);
    return gen;
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return null;
}
