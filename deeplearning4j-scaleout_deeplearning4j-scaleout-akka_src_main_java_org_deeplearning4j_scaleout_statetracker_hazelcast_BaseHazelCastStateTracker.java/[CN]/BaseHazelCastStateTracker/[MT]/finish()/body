{
  try {
    if (getCurrent() != null) {
      cachedCurrent=getCurrent();
      for (      NewUpdateListener listener : listeners)       listener.onUpdate(cachedCurrent);
    }
    done.set(true);
    updateSaver().cleanup();
  }
 catch (  Exception e) {
    log.warn("Hazelcast already shutdown...done() being called is pointless");
  }
}
