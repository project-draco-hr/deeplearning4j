{
  readStopWords();
  if (cache.vocabExists()) {
    log.info("Loading vocab...");
    cache.loadVocab();
    cache.resetWeights();
    return true;
  }
  if (vectorizer == null)   vectorizer=new TfidfVectorizer.Builder().cache(cache).iterate(docIter).iterate(sentenceIter).batchSize(batchSize).minWords(minWordFrequency).stopWords(stopWords).tokenize(tokenizerFactory).build();
  vectorizer.fit();
  setup();
  return false;
}
