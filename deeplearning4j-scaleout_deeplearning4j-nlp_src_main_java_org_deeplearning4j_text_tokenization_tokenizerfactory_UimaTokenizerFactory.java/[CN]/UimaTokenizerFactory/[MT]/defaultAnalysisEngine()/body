{
  try {
    if (defaultAnalysisEngine == null)     defaultAnalysisEngine=AnalysisEngineFactory.createEngine(AnalysisEngineFactory.createEngineDescription(SentenceAnnotator.getDescription(),TokenizerAnnotator.getDescription(),StemmerAnnotator.getDescription("English")));
    return defaultAnalysisEngine;
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
