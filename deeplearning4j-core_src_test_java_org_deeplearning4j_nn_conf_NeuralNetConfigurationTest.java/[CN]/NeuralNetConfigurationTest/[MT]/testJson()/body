{
  NeuralNetConfiguration conf=new NeuralNetConfiguration.Builder().dist(new NormalDistribution(1,1)).layer(new RBM()).useAdaGrad(false).build();
  assertFalse(conf.isUseAdaGrad());
  String json=conf.toJson();
  NeuralNetConfiguration read=NeuralNetConfiguration.fromJson(json);
  assertEquals(conf,read);
}
