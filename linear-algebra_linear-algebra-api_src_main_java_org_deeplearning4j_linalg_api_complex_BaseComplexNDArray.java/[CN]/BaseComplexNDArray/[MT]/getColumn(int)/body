{
  if (shape.length == 2) {
    if (ordering == NDArrayFactory.C) {
      IComplexNDArray ret=NDArrays.createComplex(data,new int[]{shape[0]},new int[]{stride[0]},offset + (c * 2),ordering);
      return ret;
    }
 else {
      IComplexNDArray ret=NDArrays.createComplex(data,new int[]{shape[0]},new int[]{stride[1]},offset + (c * 2),ordering);
      return ret;
    }
  }
 else   if (isColumnVector() && c == 0)   return this;
 else   throw new IllegalArgumentException("Unable to getFromOrigin column of non 2d matrix");
}
