{
  IComplexNDArray ret=NDArrays.createComplex(new int[]{1,length});
  List<INDArray> list=new ArrayList<>();
  sliceVectors(list);
  int count=0;
  for (int i=0; i < list.size(); i++) {
    for (int j=0; j < list.get(i).length(); j++)     ret.putScalar(count++,list.get(i).get(j));
  }
  return ret;
}
