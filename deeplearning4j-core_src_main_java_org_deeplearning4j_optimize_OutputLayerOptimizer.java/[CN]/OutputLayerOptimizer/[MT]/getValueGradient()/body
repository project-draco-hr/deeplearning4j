{
  OutputLayerGradient grad=logReg.getGradient(lr);
  DoubleMatrix ret=new DoubleMatrix(getNumParameters());
  for (int i=0; i < ret.length; i++) {
    if (i < logReg.getW().length)     ret.put(i,grad.getwGradient().get(i));
 else     ret.put(i,grad.getbGradient().get(i - logReg.getW().length));
  }
  return ret;
}
