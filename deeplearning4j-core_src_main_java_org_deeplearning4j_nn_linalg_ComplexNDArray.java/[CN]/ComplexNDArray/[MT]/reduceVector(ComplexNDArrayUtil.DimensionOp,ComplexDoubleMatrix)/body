{
switch (op) {
case SUM:
    return vector.sum();
case MEAN:
  return vector.mean();
case NORM_1:
return new ComplexDouble(vector.norm1());
case NORM_2:
return new ComplexDouble(vector.norm2());
case NORM_MAX:
return new ComplexDouble(vector.normmax());
case FFT:
default :
throw new IllegalArgumentException("Illegal operation");
}
}
