{
  if (isVector())   op.operate(new DimensionSlice(false,this,null));
  if (isMatrix()) {
    for (int i=0; i < rows(); i++) {
      ComplexNDArray row=getRow(i);
      op.operate(new DimensionSlice(false,row,null));
    }
  }
 else {
    for (int i=0; i < slices(); i++) {
      slice(i).iterateOverAllRows(op);
    }
  }
}
