{
  ComplexNDArray n=ComplexNDArray.wrap(this,v);
  if (!n.isVector())   throw new IllegalArgumentException("Unable to insert matrix, wrong shape " + Arrays.toString(n.shape()));
  if (n.isVector() && n.length != columns())   throw new IllegalArgumentException("Unable to put row, mis matched columns");
  for (int i=0; i < v.length; i++) {
    put(r + 2 * i * stride[0],v.get(i));
  }
}
