{
  if (cache)   return words.get(index);
  List<VocabWord> ret=new CopyOnWriteArrayList<>();
  try {
    Document doc=reader.document(index);
    String[] values=doc.getValues(WORD_FIELD);
    for (    String s : values) {
      ret.add(vocabCache.wordFor(s));
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return ret;
}
