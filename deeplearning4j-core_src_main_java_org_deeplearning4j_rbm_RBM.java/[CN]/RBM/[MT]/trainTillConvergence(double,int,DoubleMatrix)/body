{
  if (input != null && cacheInput)   this.input=input;
  if (visibleType == VisibleUnit.GAUSSIAN)   this.sigma=MatrixUtil.columnVariance(input).divi(input.rows);
  optimizer=new RBMOptimizer(this,learningRate,new Object[]{k,learningRate},optimizationAlgo,lossFunction);
  optimizer.train(input);
}
