{
  IComplexNDArray arr=new ComplexNDArray(new NDArray(DoubleMatrix.linspace(1,8,8).data,new int[]{8}));
  for (int i=0; i < arr.length(); i++) {
    IComplexNumber curr=(IComplexNumber)arr.getScalar(i).element();
    assertEquals(new ComplexDouble(i + 1),curr);
  }
  IComplexNDArray matrix=new ComplexNDArray(new NDArray(DoubleMatrix.linspace(1,8,8).data,new int[]{2,4}));
  IComplexNDArray row=matrix.getRow(1);
  IComplexNDArray column=matrix.getColumn(1);
  IComplexNDArray validate=new ComplexNDArray(new NDArray(new double[]{5,6,7,8},new int[]{4}));
  ComplexDouble d=(ComplexDouble)row.getScalar(3).element();
  assertEquals(new ComplexDouble(8),d);
  assertEquals(row,validate);
  IComplexDouble d2=(IComplexDouble)column.getScalar(1).element();
  assertEquals(new ComplexDouble(6),d2);
}
