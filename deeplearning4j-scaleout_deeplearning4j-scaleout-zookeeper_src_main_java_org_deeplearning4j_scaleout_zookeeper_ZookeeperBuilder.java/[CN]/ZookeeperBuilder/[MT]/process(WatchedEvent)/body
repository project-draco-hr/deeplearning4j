{
  if (event.getState() == KeeperState.SyncConnected) {
    log.info("Synced");
  }
 else   if (event.getState() == KeeperState.Disconnected) {
    keeper=build();
  }
 else   if (event.getState() == KeeperState.Expired) {
    keeper=build();
  }
  if (watcher != null)   watcher.process(event);
  log.info("Processed event...");
}
