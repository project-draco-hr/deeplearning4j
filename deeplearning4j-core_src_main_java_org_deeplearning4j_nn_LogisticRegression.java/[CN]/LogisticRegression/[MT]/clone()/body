{
  LogisticRegression reg=new LogisticRegression();
  reg.b=b.dup();
  reg.W=W.dup();
  reg.l2=this.l2;
  if (this.labels != null)   reg.labels=this.labels.dup();
  reg.nIn=this.nIn;
  reg.nOut=this.nOut;
  reg.useRegularization=this.useRegularization;
  if (this.input != null)   reg.input=this.input.dup();
  return reg;
}
