{
  if (concatBiases) {
    DoubleMatrix preAct=y.mmul(W.transpose());
    preAct=DoubleMatrix.concatHorizontally(preAct,DoubleMatrix.ones(preAct.rows,1));
    return sigmoid(preAct);
  }
 else {
    DoubleMatrix preAct=y.mmul(W.transpose());
    preAct.addiRowVector(vBias);
    return sigmoid(preAct);
  }
}
