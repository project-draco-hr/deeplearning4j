{
  try {
    URL url=getResource(name);
    if (url == null) {
      log.info(name + " not found");
      return null;
    }
 else {
      log.info("found resource " + name + " at "+ url);
    }
    return new InputStreamReader(url.openStream());
  }
 catch (  Exception e) {
    return null;
  }
}
