{
  String modelPath=String.format("/models/%s-token.bin","en");
  return createPrimitiveDescription(ConcurrentTokenizer.class,opennlp.uima.util.UimaUtil.MODEL_PARAMETER,ExternalResourceFactory.createExternalResourceDescription(TokenizerModelResourceImpl.class,ConcurrentTokenizer.class.getResource(modelPath).toString()),opennlp.uima.util.UimaUtil.SENTENCE_TYPE_PARAMETER,Sentence.class.getName(),opennlp.uima.util.UimaUtil.TOKEN_TYPE_PARAMETER,Token.class.getName());
}
