{
  if (!conf.isUseRegularization() || conf.getL1() <= 0.0)   return 0.0;
  double l1=Transforms.abs(getParam(LSTMParamInitializer.RECURRENT_WEIGHT_KEY)).sum(Integer.MAX_VALUE).getDouble(0) + Transforms.abs(getParam(LSTMParamInitializer.INPUT_WEIGHT_KEY)).sum(Integer.MAX_VALUE).getDouble(0);
  return conf.getL1() * l1;
}
