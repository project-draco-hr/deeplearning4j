{
  if (this == o)   return true;
  if (!(o instanceof MultiLayerConfiguration))   return false;
  MultiLayerConfiguration that=(MultiLayerConfiguration)o;
  return Double.compare(that.dampingFactor,dampingFactor) == 0 && pretrain == that.pretrain && useDropConnect == that.useDropConnect && useGaussNewtonVectorProductBackProp == that.useGaussNewtonVectorProductBackProp && useRBMPropUpAsActivations == that.useRBMPropUpAsActivations && !(confs != null ? !confs.equals(that.confs) : that.confs != null) && Arrays.equals(hiddenLayerSizes,that.hiddenLayerSizes);
}
