{
  if (shape.length == 2) {
    int offset=this.offset + c * 2;
    ComplexNDArray ret=new ComplexNDArray(data,new int[]{shape[0],1},new int[]{stride[0],2},offset);
    ret.ordering=ordering;
    return ret;
  }
 else   throw new IllegalArgumentException("Unable to getFromOrigin row of non 2d matrix");
}
