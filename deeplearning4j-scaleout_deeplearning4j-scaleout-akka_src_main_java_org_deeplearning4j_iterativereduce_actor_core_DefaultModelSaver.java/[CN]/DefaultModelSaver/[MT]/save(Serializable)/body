{
  if (!rewrite) {
    String path=file.getAbsolutePath();
    if (file.exists()) {
      file.renameTo(new File(file.getParentFile(),file.getName() + "-" + System.currentTimeMillis()));
    }
    file=new File(path);
  }
  log.info("Saving to " + file);
  SerializationUtils.saveObject(ser,file);
}
