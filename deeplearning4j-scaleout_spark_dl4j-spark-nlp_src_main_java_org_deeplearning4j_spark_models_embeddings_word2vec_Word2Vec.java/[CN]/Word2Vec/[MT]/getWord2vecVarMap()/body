{
  return new HashMap<String,Object>(){
{
      put("vectorLength",layerSize);
      put("useAdaGrad",useAdeGrad);
      put("negative",negative);
      put("window",window);
      put("alpha",learningRate.get());
      put("minAlpha",minLearningRate);
      put("iterations",numIterations);
      put("seed",seed);
      put("maxExp",MAX_EXP);
    }
  }
;
}
