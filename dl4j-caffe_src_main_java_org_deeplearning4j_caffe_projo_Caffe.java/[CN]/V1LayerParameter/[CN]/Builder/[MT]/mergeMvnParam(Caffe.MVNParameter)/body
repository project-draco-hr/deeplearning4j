{
  if (mvnParamBuilder_ == null) {
    if (((bitField0_ & 0x40000000) == 0x40000000) && mvnParam_ != Caffe.MVNParameter.getDefaultInstance()) {
      mvnParam_=Caffe.MVNParameter.newBuilder(mvnParam_).mergeFrom(value).buildPartial();
    }
 else {
      mvnParam_=value;
    }
    onChanged();
  }
 else {
    mvnParamBuilder_.mergeFrom(value);
  }
  bitField0_|=0x40000000;
  return this;
}
