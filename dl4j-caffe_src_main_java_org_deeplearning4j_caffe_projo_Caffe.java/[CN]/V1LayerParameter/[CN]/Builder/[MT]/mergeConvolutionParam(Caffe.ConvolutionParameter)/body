{
  if (convolutionParamBuilder_ == null) {
    if (((bitField0_ & 0x00010000) == 0x00010000) && convolutionParam_ != Caffe.ConvolutionParameter.getDefaultInstance()) {
      convolutionParam_=Caffe.ConvolutionParameter.newBuilder(convolutionParam_).mergeFrom(value).buildPartial();
    }
 else {
      convolutionParam_=value;
    }
    onChanged();
  }
 else {
    convolutionParamBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00010000;
  return this;
}
