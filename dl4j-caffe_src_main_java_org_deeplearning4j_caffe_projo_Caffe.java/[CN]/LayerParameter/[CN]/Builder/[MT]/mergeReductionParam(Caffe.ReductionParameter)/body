{
  if (reductionParamBuilder_ == null) {
    if (((bitField1_ & 0x00000040) == 0x00000040) && reductionParam_ != Caffe.ReductionParameter.getDefaultInstance()) {
      reductionParam_=Caffe.ReductionParameter.newBuilder(reductionParam_).mergeFrom(value).buildPartial();
    }
 else {
      reductionParam_=value;
    }
    onChanged();
  }
 else {
    reductionParamBuilder_.mergeFrom(value);
  }
  bitField1_|=0x00000040;
  return this;
}
