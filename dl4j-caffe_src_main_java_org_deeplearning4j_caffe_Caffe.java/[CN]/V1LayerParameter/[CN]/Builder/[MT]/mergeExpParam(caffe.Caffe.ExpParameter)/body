{
  if (expParamBuilder_ == null) {
    if (((bitField0_ & 0x00200000) == 0x00200000) && expParam_ != caffe.Caffe.ExpParameter.getDefaultInstance()) {
      expParam_=caffe.Caffe.ExpParameter.newBuilder(expParam_).mergeFrom(value).buildPartial();
    }
 else {
      expParam_=value;
    }
    onChanged();
  }
 else {
    expParamBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00200000;
  return this;
}
