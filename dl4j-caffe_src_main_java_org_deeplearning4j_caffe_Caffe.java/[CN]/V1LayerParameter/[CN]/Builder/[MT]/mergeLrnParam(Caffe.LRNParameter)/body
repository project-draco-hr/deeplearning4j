{
  if (lrnParamBuilder_ == null) {
    if (((bitField0_ & 0x10000000) == 0x10000000) && lrnParam_ != Caffe.LRNParameter.getDefaultInstance()) {
      lrnParam_=Caffe.LRNParameter.newBuilder(lrnParam_).mergeFrom(value).buildPartial();
    }
 else {
      lrnParam_=value;
    }
    onChanged();
  }
 else {
    lrnParamBuilder_.mergeFrom(value);
  }
  bitField0_|=0x10000000;
  return this;
}
