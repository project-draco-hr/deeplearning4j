{
  if (sigmoidParamBuilder_ == null) {
    if (((bitField1_ & 0x00000004) == 0x00000004) && sigmoidParam_ != caffe.Caffe.SigmoidParameter.getDefaultInstance()) {
      sigmoidParam_=caffe.Caffe.SigmoidParameter.newBuilder(sigmoidParam_).mergeFrom(value).buildPartial();
    }
 else {
      sigmoidParam_=value;
    }
    onChanged();
  }
 else {
    sigmoidParamBuilder_.mergeFrom(value);
  }
  bitField1_|=0x00000004;
  return this;
}
