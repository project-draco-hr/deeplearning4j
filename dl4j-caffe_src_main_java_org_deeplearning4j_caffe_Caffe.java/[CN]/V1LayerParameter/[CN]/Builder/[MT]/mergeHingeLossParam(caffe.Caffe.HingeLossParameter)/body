{
  if (hingeLossParamBuilder_ == null) {
    if (((bitField0_ & 0x01000000) == 0x01000000) && hingeLossParam_ != caffe.Caffe.HingeLossParameter.getDefaultInstance()) {
      hingeLossParam_=caffe.Caffe.HingeLossParameter.newBuilder(hingeLossParam_).mergeFrom(value).buildPartial();
    }
 else {
      hingeLossParam_=value;
    }
    onChanged();
  }
 else {
    hingeLossParamBuilder_.mergeFrom(value);
  }
  bitField0_|=0x01000000;
  return this;
}
