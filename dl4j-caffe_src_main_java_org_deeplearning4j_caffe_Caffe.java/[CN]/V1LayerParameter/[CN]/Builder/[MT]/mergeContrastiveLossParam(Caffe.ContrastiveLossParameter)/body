{
  if (contrastiveLossParamBuilder_ == null) {
    if (((bitField0_ & 0x00008000) == 0x00008000) && contrastiveLossParam_ != Caffe.ContrastiveLossParameter.getDefaultInstance()) {
      contrastiveLossParam_=Caffe.ContrastiveLossParameter.newBuilder(contrastiveLossParam_).mergeFrom(value).buildPartial();
    }
 else {
      contrastiveLossParam_=value;
    }
    onChanged();
  }
 else {
    contrastiveLossParamBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00008000;
  return this;
}
