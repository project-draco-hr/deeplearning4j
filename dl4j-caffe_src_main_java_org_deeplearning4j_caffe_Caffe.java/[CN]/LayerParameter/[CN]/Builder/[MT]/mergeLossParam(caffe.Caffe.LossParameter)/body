{
  if (lossParamBuilder_ == null) {
    if (((bitField0_ & 0x00001000) == 0x00001000) && lossParam_ != caffe.Caffe.LossParameter.getDefaultInstance()) {
      lossParam_=caffe.Caffe.LossParameter.newBuilder(lossParam_).mergeFrom(value).buildPartial();
    }
 else {
      lossParam_=value;
    }
    onChanged();
  }
 else {
    lossParamBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00001000;
  return this;
}
