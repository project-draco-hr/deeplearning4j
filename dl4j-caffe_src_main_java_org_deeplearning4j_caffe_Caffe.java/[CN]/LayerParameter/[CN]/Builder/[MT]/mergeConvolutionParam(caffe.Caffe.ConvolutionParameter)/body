{
  if (convolutionParamBuilder_ == null) {
    if (((bitField0_ & 0x00020000) == 0x00020000) && convolutionParam_ != caffe.Caffe.ConvolutionParameter.getDefaultInstance()) {
      convolutionParam_=caffe.Caffe.ConvolutionParameter.newBuilder(convolutionParam_).mergeFrom(value).buildPartial();
    }
 else {
      convolutionParam_=value;
    }
    onChanged();
  }
 else {
    convolutionParamBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00020000;
  return this;
}
