{
  if (reluParamBuilder_ == null) {
    if (((bitField1_ & 0x00000080) == 0x00000080) && reluParam_ != caffe.Caffe.ReLUParameter.getDefaultInstance()) {
      reluParam_=caffe.Caffe.ReLUParameter.newBuilder(reluParam_).mergeFrom(value).buildPartial();
    }
 else {
      reluParam_=value;
    }
    onChanged();
  }
 else {
    reluParamBuilder_.mergeFrom(value);
  }
  bitField1_|=0x00000080;
  return this;
}
