{
  if (expParamBuilder_ == null) {
    if (((bitField0_ & 0x00400000) == 0x00400000) && expParam_ != Caffe.ExpParameter.getDefaultInstance()) {
      expParam_=Caffe.ExpParameter.newBuilder(expParam_).mergeFrom(value).buildPartial();
    }
 else {
      expParam_=value;
    }
    onChanged();
  }
 else {
    expParamBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00400000;
  return this;
}
