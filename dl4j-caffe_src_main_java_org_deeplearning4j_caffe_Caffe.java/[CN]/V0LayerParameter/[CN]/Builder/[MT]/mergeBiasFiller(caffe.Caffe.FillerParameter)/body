{
  if (biasFillerBuilder_ == null) {
    if (((bitField0_ & 0x00000020) == 0x00000020) && biasFiller_ != caffe.Caffe.FillerParameter.getDefaultInstance()) {
      biasFiller_=caffe.Caffe.FillerParameter.newBuilder(biasFiller_).mergeFrom(value).buildPartial();
    }
 else {
      biasFiller_=value;
    }
    onChanged();
  }
 else {
    biasFillerBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00000020;
  return this;
}
