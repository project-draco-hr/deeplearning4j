{
  DenoisingAutoEncoder ret=new DenoisingAutoEncoder.Builder().withHBias(hbias).withInput(input).withWeights(W).withRandom(rng).withMomentum(momentum).withVisibleBias(vBias).numberOfVisible(nVisible).numHidden(nHidden).withSparsity(0.01).renderWeights(renderWeightsEveryNEpochs).fanIn(fanIn).build();
  ret.getW().muli(10000);
  return ret;
}
