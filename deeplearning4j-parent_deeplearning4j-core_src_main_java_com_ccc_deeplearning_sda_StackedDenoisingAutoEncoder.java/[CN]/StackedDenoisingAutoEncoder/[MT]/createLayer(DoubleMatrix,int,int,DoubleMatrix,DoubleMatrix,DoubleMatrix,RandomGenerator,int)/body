{
  DenoisingAutoEncoder ret=new DenoisingAutoEncoder.Builder().withHBias(hbias).withInput(input).withWeights(W).withRandom(rng).withMomentum(momentum).withVisibleBias(vBias).numberOfVisible(nVisible).numHidden(nHidden).withDistribution(dist).withSparsity(0.01).renderWeights(renderWeightsEveryNEpochs).fanIn(fanIn).build();
  return ret;
}
