{
  if (conf.getLossFunction() == LossFunctions.LossFunction.CUSTOM) {
    LossFunction create=Nd4j.getOpFactory().createLossFunction(conf.getCustomLossFunction(),input,z);
    create.exec();
    score=create.currentResult().doubleValue();
  }
 else {
    score=LossCalculation.builder().l1(1.0).l2(1.0).l1Magnitude(calcL1()).l2Magnitude(calcL2()).labels(input).z(z).lossFunction(conf.getLossFunction()).useRegularization(conf.isUseRegularization()).build().score();
  }
}
