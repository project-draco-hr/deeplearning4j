{
  int length=0;
  for (  String s : params.keySet()) {
    length+=params.get(s).length();
  }
  INDArray ret=Nd4j.create(1,length);
  int count=0;
  for (  String s : params.keySet()) {
    INDArray get=params.get(s).linearView();
    for (int i=0; i < get.length(); i++) {
      ret.putScalar(count++,get.getDouble(i));
    }
  }
  return ret;
}
