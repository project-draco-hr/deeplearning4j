{
  Layer layer=null;
  try {
    layer=getClass().newInstance();
  }
 catch (  InstantiationException e) {
    e.printStackTrace();
  }
catch (  IllegalAccessException e) {
    e.printStackTrace();
  }
  layer.setB(b.dup());
  layer.setW(W.dup());
  if (input != null)   layer.setInput(input.dup());
  return layer;
}
