{
  if (isMatrix()) {
    if (shape().length > 2)     return Shape.squeeze(shape)[0];
 else     if (shape().length == 2)     return shape[0];
  }
 else   if (isVector()) {
    if (isRowVector())     return 1;
 else     return shape[0];
  }
  throw new IllegalStateException("Unable to getFromOrigin number of of rows for a non 2d matrix");
}
