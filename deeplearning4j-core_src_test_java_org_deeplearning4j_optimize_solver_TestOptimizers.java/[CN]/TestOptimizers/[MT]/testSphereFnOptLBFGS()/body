{
  int[] numLineSearchIter={1,2,5,10};
  for (  int n : numLineSearchIter)   testSphereFnOptHelper(OptimizationAlgorithm.LBFGS,n,2);
  for (  int n : numLineSearchIter)   testSphereFnOptHelper(OptimizationAlgorithm.LBFGS,n,10);
  for (  int n : numLineSearchIter)   testSphereFnOptHelper(OptimizationAlgorithm.LBFGS,n,100);
}
