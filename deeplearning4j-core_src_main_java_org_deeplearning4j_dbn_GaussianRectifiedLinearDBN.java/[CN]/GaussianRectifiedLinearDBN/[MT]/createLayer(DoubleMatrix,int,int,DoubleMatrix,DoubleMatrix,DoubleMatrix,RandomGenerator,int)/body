{
  NeuralNetwork ret=new GaussianRectifiedLinearRBM.Builder().useRegularization(isUseRegularization()).withLossFunction(getLossFunction()).withDistribution(getDist()).useAdaGrad(isUseAdaGrad()).normalizeByInputRows(normalizeByInputRows).withHBias(hBias).numberOfVisible(nVisible).numHidden(nHidden).withSparsity(getSparsity()).withOptmizationAlgo(getOptimizationAlgorithm()).withInput(input).withL2(getL2()).fanIn(getFanIn()).renderWeights(getRenderWeightsEveryNEpochs()).withRandom(rng).withWeights(W).build();
  if (gradientListeners.get(index) != null)   ret.setGradientListeners(gradientListeners.get(index));
  return ret;
}
