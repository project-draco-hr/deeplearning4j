{
  INDArray weight=layer.getParam(DefaultParamInitializer.WEIGHT_KEY);
  graphPlotType("histogram",Arrays.asList("W","w-gradient"),new INDArray[]{layer.getParam(DefaultParamInitializer.WEIGHT_KEY),gradient},localPath + "weightHistograms.png");
  plotActivations(layer);
  renderFilter(weight,patchesPerRow);
}
