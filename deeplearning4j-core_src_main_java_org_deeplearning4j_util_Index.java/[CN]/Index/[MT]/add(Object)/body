{
  if (o instanceof String) {
    if (o.toString().isEmpty())     throw new IllegalArgumentException("Unable to add the empty string");
  }
  Integer index=indexes.get(o);
  if (index == null) {
    index=objects.size();
    objects.put(index,o);
    indexes.put(o,index);
    return true;
  }
  return false;
}
