{
  if (thresholdParamBuilder_ == null) {
    if (((bitField1_ & 0x00000040) == 0x00000040) && thresholdParam_ != Caffe.ThresholdParameter.getDefaultInstance()) {
      thresholdParam_=Caffe.ThresholdParameter.newBuilder(thresholdParam_).mergeFrom(value).buildPartial();
    }
 else {
      thresholdParam_=value;
    }
    onChanged();
  }
 else {
    thresholdParamBuilder_.mergeFrom(value);
  }
  bitField1_|=0x00000040;
  return this;
}
