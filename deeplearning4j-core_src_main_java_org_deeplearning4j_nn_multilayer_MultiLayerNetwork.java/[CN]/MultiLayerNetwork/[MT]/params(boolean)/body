{
  if (backwardOnly)   return params();
  List<INDArray> params=new ArrayList<>();
  for (  Layer layer : getLayers()) {
    INDArray layerParams;
    if (layer instanceof BasePretrainNetwork && backwardOnly)     layerParams=((BasePretrainNetwork)layer).paramsBackprop();
 else     layerParams=layer.params();
    if (layerParams != null)     params.add(layerParams);
  }
  return Nd4j.toFlattened('f',params);
}
