{
  if (layerWiseConfigurations.isPretrain()) {
    pretrain(iter);
    iter.reset();
    finetune(iter);
  }
  if (layerWiseConfigurations.isBackward()) {
    iter.reset();
    while (iter.hasNext()) {
      DataSet next=iter.next();
      setInput(next.getFeatureMatrix());
      setLabels(next.getLabels());
      doBackWard();
    }
  }
}
