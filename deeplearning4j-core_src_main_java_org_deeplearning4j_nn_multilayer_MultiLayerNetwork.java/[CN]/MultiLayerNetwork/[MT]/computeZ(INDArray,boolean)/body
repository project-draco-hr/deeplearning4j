{
  if (input == null)   throw new IllegalStateException("Unable to perform feed forward; no input found");
 else   if (this.getLayerWiseConfigurations().getInputPreProcess(0) != null)   this.input=getLayerWiseConfigurations().getInputPreProcess(0).preProcess(input);
 else   this.input=input;
  return computeZ(training);
}
