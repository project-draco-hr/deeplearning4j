{
  if (inMemoryLookupTable.getSyn0().data().dataType() == DataBuffer.DOUBLE)   Nd4j.getBlasWrapper().axpy(alpha.doubleValue(),getFrom(inMemoryLookupTable),getTo(inMemoryLookupTable));
 else   Nd4j.getBlasWrapper().axpy(alpha.floatValue(),getFrom(inMemoryLookupTable),getTo(inMemoryLookupTable));
  return inMemoryLookupTable;
}
