{
  JCublasNDArray cA=(JCublasNDArray)A;
  JCublasNDArray cB=(JCublasNDArray)B;
  JCublasNDArray cC=(JCublasNDArray)C;
  char trans='n';
  if (A.rows() == B.columns()) {
    trans='T';
  }
  JCublas.cublasDgemv('n',A.rows(),A.columns(),alpha,cA.pointer(),A.rows(),cB.pointer(),B.rows(),beta,cC.pointer(),cC.stride()[0]);
  cC.getData();
  return C;
}
