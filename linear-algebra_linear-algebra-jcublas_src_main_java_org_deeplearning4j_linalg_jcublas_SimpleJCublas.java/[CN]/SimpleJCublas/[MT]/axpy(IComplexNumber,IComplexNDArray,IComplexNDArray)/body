{
  JCublasComplexNDArray aC=(JCublasComplexNDArray)A;
  JCublasComplexNDArray bC=(JCublasComplexNDArray)B;
  JCublas.cublasCaxpy(aC.length(),jcuda.cuComplex.cuCmplx(da.realComponent().floatValue(),da.imaginaryComponent().floatValue()),aC.pointer(),A.stride()[0],bC.pointer(),B.stride()[0]);
  ((JCublasComplexNDArray)B).getData();
}
