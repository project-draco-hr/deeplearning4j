{
  JCublasComplexNDArray aC=(JCublasComplexNDArray)A;
  JCublasComplexNDArray bC=(JCublasComplexNDArray)B;
  alloc(aC,bC);
  JCublas.cublasCaxpy(aC.length(),jcuda.cuComplex.cuCmplx(da.realComponent().floatValue(),da.imaginaryComponent().floatValue()),aC.pointer(),1,bC.pointer(),1);
  ((JCublasComplexNDArray)B).getData();
  free(aC,bC);
}
