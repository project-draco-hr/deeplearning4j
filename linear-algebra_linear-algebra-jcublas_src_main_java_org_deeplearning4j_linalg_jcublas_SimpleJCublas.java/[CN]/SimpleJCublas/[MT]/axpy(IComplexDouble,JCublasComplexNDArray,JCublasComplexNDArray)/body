{
  Pointer d_A=new Pointer();
  Pointer d_B=new Pointer();
  int length=A.length();
  int length_o=B.length();
  if (length != length_o)   return;
  init();
  TwoPointersVi(d_A,d_B,A,B);
  JCublas.cublasZaxpy(length,jcuda.cuDoubleComplex.cuCmplx(da.realComponent(),da.imaginaryComponent()),d_A,1,d_B,1);
  gvi(d_B,B);
  JCublas.cublasShutdown();
  JCublas.cublasFree(d_A);
  JCublas.cublasFree(d_B);
}
