{
  try {
    if (path.getAbsolutePath().endsWith(".ser"))     vectors=SerializationUtils.readObject(path);
 else {
      vectors=WordVectorSerializer.fromPair(WordVectorSerializer.loadTxt(path));
    }
    vectors.setModelUtils(new BasicModelUtils());
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
