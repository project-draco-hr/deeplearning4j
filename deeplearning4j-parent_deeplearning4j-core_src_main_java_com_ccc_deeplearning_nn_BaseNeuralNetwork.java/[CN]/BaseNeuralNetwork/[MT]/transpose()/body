{
  try {
    NeuralNetwork ret=getClass().newInstance();
    ret.sethBias(hBias.dup());
    ret.setvBias(vBias.dup());
    ret.setnHidden(getnVisible());
    ret.setnVisible(getnHidden());
    ret.setW(W.transpose());
    ret.setRng(getRng());
    return ret;
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
