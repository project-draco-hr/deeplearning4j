{
  int split=conf.getInt(SPLIT);
  List<List<Pair<DoubleMatrix,DoubleMatrix>>> splitList=Lists.partition(pairs,split);
  partition=splitList.size();
  for (int i=0; i < splitList.size(); i++)   mediator.tell(new DistributedPubSubMediator.Publish(BROADCAST,new ArrayList<>(splitList.get(i))),getSelf());
}
