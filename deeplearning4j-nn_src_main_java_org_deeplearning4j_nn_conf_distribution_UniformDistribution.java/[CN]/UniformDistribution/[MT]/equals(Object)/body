{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  UniformDistribution other=(UniformDistribution)obj;
  if (Double.doubleToLongBits(lower) != Double.doubleToLongBits(other.lower))   return false;
  if (Double.doubleToLongBits(upper) != Double.doubleToLongBits(other.upper))   return false;
  return true;
}
