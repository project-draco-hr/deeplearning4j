{
  int[] ret=new int[inputWidthAndHeight.length];
  for (int i=0; i < ret.length; i++) {
    if (kernelWidthAndHeight[i] == 1)     ret[i]=inputWidthAndHeight[i] / stride[i];
 else {
      ret[i]=(inputWidthAndHeight[i] - kernelWidthAndHeight[i]) / stride[i] + 1;
    }
  }
  lastHeight=ret[0];
  lastWidth=ret[1];
}
