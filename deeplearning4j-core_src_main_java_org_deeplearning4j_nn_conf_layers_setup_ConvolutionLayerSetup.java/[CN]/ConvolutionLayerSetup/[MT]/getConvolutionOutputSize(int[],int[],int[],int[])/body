{
  int[] ret=new int[input.length];
  new KernelValidationUtil().validateShapes(input[0],input[1],kernel[0],kernel[1],stride[0],stride[1],padding[0],padding[1]);
  for (int i=0; i < ret.length; i++) {
    ret[i]=(input[i] - kernel[i] + (2 * padding[i])) / stride[i] + 1;
  }
  lastHeight=ret[0];
  lastWidth=ret[1];
}
