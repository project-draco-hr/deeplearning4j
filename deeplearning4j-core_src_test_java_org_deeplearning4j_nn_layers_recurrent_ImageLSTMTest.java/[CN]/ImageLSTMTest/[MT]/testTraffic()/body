{
  NeuralNetConfiguration conf=new NeuralNetConfiguration.Builder().layer(new org.deeplearning4j.nn.conf.layers.ImageLSTM.Builder().nIn(4).nOut(4).activation("tanh").build()).optimizationAlgo(OptimizationAlgorithm.LBFGS).build();
  Layer l=LayerFactories.getFactory(conf.getLayer()).create(conf,Arrays.<IterationListener>asList(new ScoreIterationListener(10)),0);
}
