{
  if (newEpoch || epochs == numEpochs) {
    log.info("Epoch " + epochs + ", number of batches completed "+ lastBatch);
    newEpoch=false;
  }
  if (iter == null)   return (epochs < numEpochs) && ((batchedDS.size() != 0 && batchedDS.size() > batch) || batchedDS.size() == 0);
 else   return (epochs < numEpochs) || (iter.hasNext() && (epochs == 0 || epochs == numEpochs));
}
