{
  if (!iter.hasNext()) {
    passes++;
    log.info("Epoch " + passes + ", number of batches completed "+ batch);
    if (passes < numPasses) {
      batch=0;
      iter.reset();
    }
  }
  batch++;
  DataSet next=iter.next(num);
  if (preProcessor != null)   preProcessor.preProcess(next);
  return next;
}
