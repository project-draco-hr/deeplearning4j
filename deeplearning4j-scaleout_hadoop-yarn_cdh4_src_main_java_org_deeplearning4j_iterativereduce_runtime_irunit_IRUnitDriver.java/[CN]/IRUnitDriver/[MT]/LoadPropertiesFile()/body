{
  this.props=new Properties();
  try {
    FileInputStream fis=new FileInputStream(this.app_properties_file);
    props.load(fis);
    fis.close();
  }
 catch (  FileNotFoundException ex) {
    log.error("Error with file",ex);
  }
catch (  IOException ex) {
    log.error("Error with file",ex);
  }
}
