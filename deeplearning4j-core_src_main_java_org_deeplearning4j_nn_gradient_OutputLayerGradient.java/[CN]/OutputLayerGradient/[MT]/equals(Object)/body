{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  OutputLayerGradient other=(OutputLayerGradient)obj;
  if (bGradient == null) {
    if (other.bGradient != null)     return false;
  }
 else   if (!bGradient.equals(other.bGradient))   return false;
  if (wGradient == null) {
    if (other.wGradient != null)     return false;
  }
 else   if (!wGradient.equals(other.wGradient))   return false;
  return true;
}
