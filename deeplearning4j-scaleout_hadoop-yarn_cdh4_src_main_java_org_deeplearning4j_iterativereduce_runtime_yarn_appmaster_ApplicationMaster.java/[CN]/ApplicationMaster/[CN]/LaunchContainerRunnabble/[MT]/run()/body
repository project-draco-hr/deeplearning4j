{
  LOG.debug("Launching container for worker=" + workerId + ", container="+ container);
  cmHandler=new ContainerManagerHandler(conf,container);
  cmHandler.getContainerManager();
  try {
    Map<String,LocalResource> localResources=Utils.getLocalResourcesForApplication(conf,appAttemptId.getApplicationId(),appName,props,LocalResourceVisibility.APPLICATION);
    List<String> commands=Utils.getWorkerCommand(conf,props,masterHost + ":" + masterPort,workerId);
    cmHandler.startContainer(commands,localResources,Utils.getEnvironment(conf,props));
    cmHandler.getContainerStatus();
  }
 catch (  IOException ex) {
  }
}
