{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  GaussianDistribution other=(GaussianDistribution)obj;
  if (Double.doubleToLongBits(mean) != Double.doubleToLongBits(other.mean))   return false;
  if (Double.doubleToLongBits(std) != Double.doubleToLongBits(other.std))   return false;
  return true;
}
