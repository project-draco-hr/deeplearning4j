{
  DenoisingAutoEncoder ret=new DenoisingAutoEncoder.Builder().withDropOut(dropOut).withHBias(hbias).withInput(input).withWeights(W).withDistribution(getDist()).withOptmizationAlgo(getOptimizationAlgorithm()).withRandom(rng).withMomentum(getMomentum()).withVisibleBias(vBias).normalizeByInputRows(normalizeByInputRows).numberOfVisible(nVisible).numHidden(nHidden).withDistribution(getDist()).withLossFunction(getLossFunction()).withSparsity(this.getSparsity()).renderWeights(getRenderWeightsEveryNEpochs()).fanIn(getFanIn()).build();
  return ret;
}
