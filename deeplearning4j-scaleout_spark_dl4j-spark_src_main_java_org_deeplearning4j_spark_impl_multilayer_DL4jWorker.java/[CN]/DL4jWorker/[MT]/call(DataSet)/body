{
  MultiLayerNetwork network=init();
  try {
    network.initialize(v1);
    network.fit(v1);
    INDArray params=network.params();
    return params;
  }
 catch (  Exception e) {
    e.printStackTrace();
    System.err.println("Error with input " + Arrays.toString(v1.getFeatureMatrix().shape()));
    throw e;
  }
}
