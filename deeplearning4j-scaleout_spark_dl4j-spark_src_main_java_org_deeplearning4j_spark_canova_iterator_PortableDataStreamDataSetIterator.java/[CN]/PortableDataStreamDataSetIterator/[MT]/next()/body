{
  DataSet ds;
  if (preloadedDataSet != null) {
    ds=preloadedDataSet;
    preloadedDataSet=null;
  }
 else {
    ds=load(iter.next());
  }
  totalOutcomes=ds.numOutcomes();
  inputColumns=ds.numInputs();
  batch=ds.numExamples();
  if (preprocessor != null)   preprocessor.preProcess(ds);
  return ds;
}
