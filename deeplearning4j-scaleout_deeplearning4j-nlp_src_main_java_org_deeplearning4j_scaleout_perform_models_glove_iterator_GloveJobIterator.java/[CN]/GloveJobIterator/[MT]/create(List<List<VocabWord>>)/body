{
  if (cache == null)   throw new IllegalStateException("Unable to create work; no vocab found");
  if (table == null)   throw new IllegalStateException("Unable to create work; no table found");
  if (sentence == null)   throw new IllegalArgumentException("Unable to create work from null sentence");
  Word2VecWork work=new Word2VecWork((InMemoryLookupTable)table,(InMemoryLookupCache)cache,sentence);
  return work;
}
