{
  for (int n=0; n < size; n++) {
    INDArray point=data.slice(n);
    if (!boundary.contains(point))     return false;
  }
  if (!isLeaf) {
    boolean correct=true;
    for (int i=0; i < numChildren; i++)     correct=correct && children.get(i).isCorrect();
    return correct;
  }
 else   return true;
}
