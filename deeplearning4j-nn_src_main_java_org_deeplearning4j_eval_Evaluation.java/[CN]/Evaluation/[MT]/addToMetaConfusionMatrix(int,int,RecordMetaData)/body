{
  if (confusionMatrixMeta == null) {
    confusionMatrixMeta=new HashMap<>();
  }
  Pair<Integer,Integer> p=new Pair<>(actual,predicted);
  List<RecordMetaData> list=confusionMatrixMeta.get(p);
  if (list == null) {
    list=new ArrayList<>();
    confusionMatrixMeta.put(p,list);
  }
  list.add(metaData);
}
