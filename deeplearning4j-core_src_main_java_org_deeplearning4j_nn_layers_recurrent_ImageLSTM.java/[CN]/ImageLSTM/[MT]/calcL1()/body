{
  if (!conf.isUseRegularization() || conf.getLayer().getL1() <= 0.0)   return 0.0;
  double l1=getParam(ImageLSTMParamInitializer.RECURRENT_WEIGHT_KEY).norm1Number().doubleValue() + getParam(ImageLSTMParamInitializer.INPUT_WEIGHT_KEY).norm1Number().doubleValue();
  return conf.getLayer().getL1() * l1;
}
