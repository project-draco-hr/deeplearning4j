{
  RNTN rntn=new RNTN.Builder().setActivationFunction(Activations.tanh()).setAdagradResetFrequency(1).setCombineClassification(true).setFeatureVectors(vec).setRandomFeatureVectors(false).setRng(new MersenneTwister(123)).setUseTensors(true).setNumHidden(25).build();
  List<Tree> trees=vectorizer.getTrees("This is one sentence.");
  rntn.train(trees);
}
