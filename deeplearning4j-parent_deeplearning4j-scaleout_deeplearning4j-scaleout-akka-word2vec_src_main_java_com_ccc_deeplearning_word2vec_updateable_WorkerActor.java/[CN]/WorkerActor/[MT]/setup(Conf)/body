{
  super.setup(conf);
  RandomGenerator rng=new MersenneTwister(conf.getLong(SEED));
  network=new Word2VecMultiLayerNetwork.Builder().withWord2Vec(vec).withLabels(labels).withActivation(new HardTanh()).numberOfInputs(numVisible).numberOfOutPuts(numHidden).hiddenLayerSizes(hiddenLayerSizes).withRng(rng).withClazz(conf.getClazz(CLASS)).build();
}
