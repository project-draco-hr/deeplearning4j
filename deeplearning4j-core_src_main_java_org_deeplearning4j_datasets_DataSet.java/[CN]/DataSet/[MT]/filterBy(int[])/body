{
  List<DataSet> list=asList();
  List<DataSet> newList=new ArrayList<>();
  List<Integer> labelList=new ArrayList<>();
  for (  int i : labels)   labelList.add(i);
  for (  DataSet d : list) {
    if (labelList.contains(d.getLabel(d))) {
      newList.add(d);
    }
  }
  return DataSet.merge(newList);
}
