{
  super.setup(conf);
  RandomGenerator rng=new SynchronizedRandomGenerator(new MersenneTwister(conf.getSeed()));
  network=new BaseMultiLayerNetwork.Builder<>().numberOfInputs(numVisible).numberOfOutPuts(numHidden).hiddenLayerSizes(hiddenLayerSizes).withRng(rng).withClazz(conf.getMultiLayerClazz()).build();
}
