{
  List<float[]> gradient=new ArrayList<>();
  for (  Iterator<? extends FloatMatrix> iter : matrices) {
    while (iter.hasNext()) {
      FloatMatrix d=iter.next();
      gradient.add(d.data);
    }
  }
  FloatMatrix ret2=new FloatMatrix(ArrayUtil.combineFloat(gradient));
  return ret2.reshape(1,ret2.length);
}
