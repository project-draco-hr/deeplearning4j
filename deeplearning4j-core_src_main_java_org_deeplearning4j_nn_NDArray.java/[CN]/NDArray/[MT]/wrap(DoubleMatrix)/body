{
  if (toWrap instanceof NDArray)   return (NDArray)toWrap;
  int[] shape;
  if (toWrap.isColumnVector())   shape=new int[]{toWrap.rows};
 else   if (toWrap.isRowVector())   shape=new int[]{toWrap.columns};
 else   shape=new int[]{toWrap.rows,toWrap.columns};
  NDArray ret=new NDArray(toWrap.data,shape,ArrayUtil.calcStrides(shape));
  return ret;
}
