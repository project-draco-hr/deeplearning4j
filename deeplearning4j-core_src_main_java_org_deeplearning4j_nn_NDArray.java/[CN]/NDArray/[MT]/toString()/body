{
  if (shape.length == 1) {
    return Double.toString(get(0));
  }
  StringBuilder sb=new StringBuilder();
  int length=shape[0];
  sb.append('[');
  if (length > 0) {
    sb.append(slice(0).toString());
    for (int i=1; i < length; i++) {
      sb.append(',');
      sb.append(slice(i).toString());
    }
  }
  sb.append(']');
  return sb.toString();
}
