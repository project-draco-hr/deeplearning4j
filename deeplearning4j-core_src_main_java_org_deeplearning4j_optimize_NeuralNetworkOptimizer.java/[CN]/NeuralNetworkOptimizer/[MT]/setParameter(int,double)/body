{
  if (index >= network.getW().length) {
    if (index >= network.getvBias().length + network.getW().length) {
      int i=getAdjustedIndex(index);
      network.gethBias().put(i,value);
    }
 else {
      int i=getAdjustedIndex(index);
      network.getvBias().put(i,value);
    }
  }
 else {
    network.getW().put(index,value);
  }
}
