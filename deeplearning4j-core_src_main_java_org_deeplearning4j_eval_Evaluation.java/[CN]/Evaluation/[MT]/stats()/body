{
  StringBuilder builder=new StringBuilder().append("\n");
  Set<Integer> classes=confusion.getClasses();
  for (  Integer clazz : classes) {
    for (    Integer clazz2 : classes) {
      int count=confusion.getCount(clazz,clazz2);
      if (count != 0)       builder.append("\nActual Class " + clazz + " was predicted with Predicted "+ clazz2+ " with count "+ count+ " times\n");
    }
  }
  builder.append("\n==========================F1 Scores========================================");
  builder.append("\n " + f1());
  builder.append("\n===========================================================================");
  return builder.toString();
}
