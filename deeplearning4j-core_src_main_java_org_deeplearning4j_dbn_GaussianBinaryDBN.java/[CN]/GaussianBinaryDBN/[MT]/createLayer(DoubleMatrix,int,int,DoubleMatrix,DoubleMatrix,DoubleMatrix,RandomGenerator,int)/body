{
  NeuralNetwork ret=null;
  if (index == 0)   ret=new GaussianBinaryRBM.Builder().useRegularization(isUseRegularization()).withDistribution(getDist()).useAdaGrad(isUseAdaGrad()).normalizeByInputRows(normalizeByInputRows).withHBias(hBias).numberOfVisible(nVisible).numHidden(nHidden).withSparsity(getSparsity()).withInput(input).withL2(getL2()).fanIn(getFanIn()).renderWeights(getRenderWeightsEveryNEpochs()).withRandom(rng).withWeights(W).build();
 else   ret=new RBM.Builder().useAdaGrad(isUseAdaGrad()).normalizeByInputRows(normalizeByInputRows).useRegularization(isUseRegularization()).withDistribution(getDist()).withHBias(hBias).numberOfVisible(nVisible).numHidden(nHidden).withSparsity(getSparsity()).withInput(input).withL2(getL2()).fanIn(getFanIn()).renderWeights(getRenderWeightsEveryNEpochs()).withRandom(rng).withWeights(W).build();
  if (gradientListeners.get(index) != null)   ret.setGradientListeners(gradientListeners.get(index));
  return ret;
}
