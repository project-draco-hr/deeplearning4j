{
  Properties result=new Properties();
  String[] props=str.trim().split(",\\s*");
  for (int i=0; i < props.length; i++) {
    String term=props[i];
    int divLoc=term.indexOf("=");
    String key;
    String value;
    if (divLoc >= 0) {
      key=term.substring(0,divLoc);
      value=term.substring(divLoc + 1);
    }
 else {
      key=term;
      value="true";
    }
    result.setProperty(key,value);
  }
  return result;
}
