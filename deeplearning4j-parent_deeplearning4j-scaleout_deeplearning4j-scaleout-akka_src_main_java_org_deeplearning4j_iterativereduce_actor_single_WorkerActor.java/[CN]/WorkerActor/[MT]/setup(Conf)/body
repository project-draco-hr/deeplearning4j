{
  super.setup(conf);
  RandomGenerator rng=new MersenneTwister(conf.getSeed());
  network=new BaseNeuralNetwork.Builder<>().numberOfVisible(numVisible).numHidden(numHidden).withRandom(rng).useRegularization(useRegularization).withClazz((Class<? extends BaseNeuralNetwork>)conf.getNeuralNetworkClazz()).build();
}
