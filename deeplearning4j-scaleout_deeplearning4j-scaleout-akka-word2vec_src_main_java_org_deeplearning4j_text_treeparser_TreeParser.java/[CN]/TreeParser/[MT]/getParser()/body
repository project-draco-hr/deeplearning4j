{
  return createEngine(createEngineDescription(SentenceAnnotator.getDescription(),TokenizerAnnotator.getDescription(),PoStagger.getDescription("en"),StemmerAnnotator.getDescription("English"),createEngineDescription(ParserAnnotator.class,ParserAnnotator.PARAM_USE_TAGS_FROM_CAS,true,ParserAnnotator.PARAM_PARSER_MODEL_PATH,ParamUtil.getParameterValue(ParserAnnotator.PARAM_PARSER_MODEL_PATH,"/models/en-parser-chunking.bin"),ParserAnnotator.PARAM_OUTPUT_TYPES_HELPER_CLASS_NAME,DefaultOutputTypesHelper.class.getName())));
}
