{
  if (graph == null) {
    graph=new Graph<VocabWord,Void>(10,false,new AbstractVertexFactory<VocabWord>());
    for (int i=0; i < 10; i++) {
      graph.getVertex(i).setValue(new VocabWord(i,String.valueOf(i)));
      int x=i + 3;
      if (x >= 10)       x=0;
      graph.addEdge(i,x,null,true);
    }
  }
}
