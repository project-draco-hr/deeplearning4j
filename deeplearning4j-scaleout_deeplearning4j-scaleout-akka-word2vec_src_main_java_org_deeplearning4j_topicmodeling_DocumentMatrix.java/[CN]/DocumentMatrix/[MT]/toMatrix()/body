{
  if (initializedWithCounter) {
    INDArray ret=NDArrays.create(words.size());
    int count=0;
    for (    String s : words.keySet())     ret.putScalar(count++,docWords.getCount(s));
    return ret;
  }
 else {
    INDArray ret=NDArrays.create(words.size());
    int count=0;
    for (    String s : words.keySet())     ret.putScalar(count++,words.getCount(s));
    return ret;
  }
}
