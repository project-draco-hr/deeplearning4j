{
  if (initializedWithCounter) {
    DoubleMatrix ret=new DoubleMatrix(words.size());
    int count=0;
    for (    String s : words.keySet())     ret.put(count++,docWords.getProbability(s));
    return ret;
  }
 else {
    DoubleMatrix ret=new DoubleMatrix(words.size());
    int count=0;
    for (    String s : words.keySet())     ret.put(count++,words.getProbability(s));
    return ret;
  }
}
