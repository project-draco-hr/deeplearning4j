{
  MultiLayerConfiguration conf=new NeuralNetConfiguration.Builder().layerFactory(LayerFactories.getFactory(RBM.class)).list(4).hiddenLayerSizes(new int[]{3,2,2}).build();
  String json=conf.toJson();
  System.out.println(json);
  MultiLayerConfiguration from=MultiLayerConfiguration.fromJson(json);
  assertEquals(conf,from);
}
