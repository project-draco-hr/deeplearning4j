{
  MultiLayerConfiguration conf=new NeuralNetConfiguration.Builder().nIn(4).nOut(3).layerFactory(LayerFactories.getFactory(RBM.class)).list(3).hiddenLayerSizes(new int[]{2,2}).build();
  String json=conf.toJson();
  System.out.println(json);
  MultiLayerConfiguration from=MultiLayerConfiguration.fromJson(json);
  assertEquals(conf,from);
}
