{
  if (!backwards)   return super.numParams(backwards);
  int ret=0;
  for (  String s : paramTable().keySet()) {
    if (backwards) {
      if (!s.equals(PretrainParamInitializer.VISIBLE_BIAS_KEY)) {
        ret+=getParam(s).length();
      }
    }
 else {
      ret+=getParam(s).length();
    }
  }
  return ret;
}
