{
  FloatMatrix ret=new FloatMatrix(rows(),columns());
  for (int i=0; i < ret.rows; i++) {
    for (int j=0; j < ret.columns; j++) {
      float sum=0;
      for (int slice=0; slice < slices(); slice++) {
        sum+=getSlice(slice).get(i,j);
      }
      ret.put(i,j,sum);
    }
  }
  return ret;
}
