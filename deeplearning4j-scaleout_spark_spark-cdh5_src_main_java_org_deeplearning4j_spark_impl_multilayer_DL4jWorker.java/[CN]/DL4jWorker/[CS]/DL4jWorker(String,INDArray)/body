{
  this.network=new MultiLayerNetwork(MultiLayerConfiguration.fromJson(json));
  this.network.init();
  int numParams=this.network.numParams();
  if (numParams != params.length())   throw new IllegalStateException("Number of params for configured network was " + numParams + " whhile the specified parameter vector length was "+ params.length());
  this.network.setParameters(params);
}
