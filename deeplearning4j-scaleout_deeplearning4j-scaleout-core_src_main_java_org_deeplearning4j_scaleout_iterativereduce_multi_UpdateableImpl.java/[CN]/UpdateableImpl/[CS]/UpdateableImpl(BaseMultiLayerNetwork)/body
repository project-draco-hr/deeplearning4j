{
  if (wrapped == null)   throw new IllegalArgumentException("Null argument not allowed");
  if (wrapped.getNeuralNets() == null || wrapped.getNeuralNets()[0] == null)   throw new IllegalArgumentException("No neuralNets found!");
  if (wrapped.getNeuralNets() == null || wrapped.getNeuralNets()[0] == null)   throw new IllegalArgumentException("No sigmoid neuralNets found");
  this.wrapped=wrapped;
  if (clazz == null)   clazz=wrapped.getClass();
}
