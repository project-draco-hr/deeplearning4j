{
  if (wrapped == null)   throw new IllegalArgumentException("Null argument not allowed");
  if (wrapped.getLayers() == null || wrapped.getLayers()[0] == null)   throw new IllegalArgumentException("No layers found!");
  if (wrapped.getSigmoidHiddeenLayers() == null || wrapped.getSigmoidHiddeenLayers()[0] == null)   throw new IllegalArgumentException("No sigmoid layers found");
  this.wrapped=wrapped;
  if (clazz == null)   clazz=wrapped.getClass();
}
