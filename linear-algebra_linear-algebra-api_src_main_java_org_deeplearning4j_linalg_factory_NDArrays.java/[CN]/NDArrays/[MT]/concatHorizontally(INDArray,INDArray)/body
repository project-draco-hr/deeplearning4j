{
  if (A.rows() != B.rows()) {
    throw new IllegalArgumentException("Matrices don't have same number of rows.");
  }
  INDArray result=NDArrays.create(A.rows(),A.columns() + B.columns());
  copy(A,result);
  int count=0;
  for (int i=A.rows(); i < B.rows(); i++) {
    result.putRow(i,B.getRow(count++));
  }
  return result;
}
