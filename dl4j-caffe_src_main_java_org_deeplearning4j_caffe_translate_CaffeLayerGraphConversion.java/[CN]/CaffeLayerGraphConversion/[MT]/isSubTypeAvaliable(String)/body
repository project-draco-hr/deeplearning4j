{
  if (!isSupported(subType)) {
    throw new IllegalArgumentException(String.format("The LayerSubType %s is not " + "supported or does not exist.",subType));
  }
 else {
    try {
      LayerSubType.valueOf(subType);
      return true;
    }
 catch (    IllegalArgumentException e) {
      return false;
    }
  }
}
