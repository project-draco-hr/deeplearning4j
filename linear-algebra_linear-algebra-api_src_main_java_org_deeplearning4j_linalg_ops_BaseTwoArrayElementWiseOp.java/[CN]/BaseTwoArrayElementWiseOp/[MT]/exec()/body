{
  if (to == null) {
    if (scalarValue != null)     for (int i=0; i < from.length(); i++)     if (scalarValue != null)     applyTransformToOrigin(i);
 else     applyTransformToOrigin(i,scalarValue);
  }
 else {
    for (int i=0; i < to.length(); i++) {
      applyTransformToDestination(i);
    }
  }
}
