{
  if (to == null) {
    if (!Double.isInfinite(scalarValue))     for (int i=0; i < from.length; i++)     if (Double.isInfinite(scalarValue))     applyTransformToOrigin(i);
 else     applyTransformToOrigin(i,scalarValue);
  }
 else {
    for (int i=0; i < to.length; i++) {
      applyTransformToDestination(i);
    }
  }
}
