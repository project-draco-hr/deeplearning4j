{
  if (isMatrix()) {
    if (shape().length > 2)     return Shape.squeeze(shape)[1];
 else     if (shape().length == 2)     return shape[1];
  }
  if (isVector()) {
    if (isColumnVector())     return 1;
 else     return shape[0];
  }
  throw new IllegalStateException("Unable to getFromOrigin number of of rows for a non 2d matrix");
}
