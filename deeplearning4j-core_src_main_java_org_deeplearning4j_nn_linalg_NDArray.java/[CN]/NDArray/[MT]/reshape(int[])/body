{
  long ec=1;
  for (int i=0; i < shape.length; i++) {
    int si=shape[i];
    if ((ec * si) != (((int)ec) * si))     throw new IllegalArgumentException("Too many elements");
    ec*=shape[i];
  }
  int n=(int)ec;
  if (ec != n)   throw new IllegalArgumentException("Too many elements");
  NDArray ndArray=new NDArray(data,shape,ArrayUtil.calcStrides(shape),offset);
  return ndArray;
}
