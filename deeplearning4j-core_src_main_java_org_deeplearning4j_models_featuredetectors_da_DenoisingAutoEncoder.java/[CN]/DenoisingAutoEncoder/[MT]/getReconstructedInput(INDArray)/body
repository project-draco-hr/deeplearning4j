{
  if (conf.isConcatBiases()) {
    INDArray preAct=y.mmul(W.transpose());
    preAct=Nd4j.concatHorizontally(preAct,Nd4j.ones(preAct.rows(),1));
    return Transforms.sigmoid(preAct);
  }
 else {
    INDArray preAct=y.mmul(W.transpose());
    preAct.addiRowVector(vBias);
    return Transforms.sigmoid(preAct);
  }
}
