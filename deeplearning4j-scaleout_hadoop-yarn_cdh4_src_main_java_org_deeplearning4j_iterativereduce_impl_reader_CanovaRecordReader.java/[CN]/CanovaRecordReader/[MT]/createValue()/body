{
  if (recordReader.hasNext()) {
    Collection<org.canova.api.writable.Writable> writables=recordReader.next();
    Collection<Writable> wrapped=new ArrayList<>();
    for (    org.canova.api.writable.Writable writable : writables)     wrapped.add(new CanovaWritableDelegate(writable));
    currRecord=wrapped;
  }
  return currRecord;
}
