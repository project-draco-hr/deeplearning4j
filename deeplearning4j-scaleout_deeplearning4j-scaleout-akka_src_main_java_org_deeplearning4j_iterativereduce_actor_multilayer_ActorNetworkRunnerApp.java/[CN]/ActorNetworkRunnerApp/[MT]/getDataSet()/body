{
  if (type.equals("worker"))   return;
  try {
    if (this.numExamples >= 0 && this.split != 0)     this.iter=(DataSetIterator)Class.forName(dataSet).getConstructor(Integer.class,Integer.class).newInstance(split,numExamples);
 else     this.iter=(DataSetIterator)Class.forName(dataSet).newInstance();
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
