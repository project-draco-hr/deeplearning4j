{
  MultiLayerConfiguration conf=new NeuralNetConfiguration.Builder().seed(12345l).list(2).layer(0,new RBM.Builder().nIn(2).nOut(2).weightInit(WeightInit.DISTRIBUTION).dist(new NormalDistribution(0,1)).build()).layer(1,new OutputLayer.Builder().nIn(2).nOut(1).weightInit(WeightInit.DISTRIBUTION).dist(new NormalDistribution(0,1)).build()).build();
  return conf;
}
