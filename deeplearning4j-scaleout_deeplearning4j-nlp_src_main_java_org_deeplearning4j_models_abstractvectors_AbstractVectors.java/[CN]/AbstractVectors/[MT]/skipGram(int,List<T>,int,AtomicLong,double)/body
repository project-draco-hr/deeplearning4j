{
  final T word=sentence.get(i);
  if (word == null || sentence.isEmpty())   return;
  int end=window * 2 + 1 - b;
  for (int a=b; a < end; a++) {
    if (a != window) {
      int c=i - window + a;
      if (c >= 0 && c < sentence.size()) {
        T lastWord=sentence.get(c);
        iterate(word,lastWord,nextRandom,alpha);
      }
    }
  }
}
