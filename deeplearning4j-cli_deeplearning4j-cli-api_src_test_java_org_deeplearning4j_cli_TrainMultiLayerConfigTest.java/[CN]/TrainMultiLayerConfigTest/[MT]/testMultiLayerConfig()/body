{
  Model testModelFlag=new Model();
  MultiLayerConfiguration conf=new NeuralNetConfiguration.Builder().layerFactory(LayerFactories.getFactory(RBM.class)).dist(Nd4j.getDistributions().createNormal(1e-1,0)).list(4).preProcessor(0,new ConvolutionPostProcessor()).hiddenLayerSizes(3,2,2).build();
  String json=conf.toJson();
  FileUtils.writeStringToFile(new File("model_multi.json"),json);
  MultiLayerConfiguration from=testModelFlag.value("model_multi.json");
  assertEquals(conf,from);
  String[] cmd={"--input","iris.txt","--model","model_multi.json","--output","model_results.txt"};
  Train train=new Train(cmd);
}
