{
  super.setup(conf);
  RandomGenerator rng=new SynchronizedRandomGenerator(new MersenneTwister(conf.getSeed()));
  network=new BaseMultiLayerNetwork.Builder<>().useRegularization(conf.isUseRegularization()).numberOfInputs(numVisible).numberOfOutPuts(numHidden).withActivation(conf.getFunction()).hiddenLayerSizes(hiddenLayerSizes).withRng(rng).transformWeightsAt(conf.getWeightTransforms()).withClazz(conf.getMultiLayerClazz()).build();
}
