{
  RBM ret=new RBM.Builder().useRegularization(isUseRegularization()).useAdaGrad(isUseAdaGrad()).normalizeByInputRows(isNormalizeByInputRows()).withMomentum(getMomentum()).withSparsity(getSparsity()).withDistribution(getDist()).numberOfVisible(nVisible).numHidden(nHidden).withWeights(W).withDropOut(dropOut).withInput(input).withVisibleBias(vBias).withHBias(hBias).withDistribution(getDist()).withRandom(rng).renderWeights(getRenderWeightsEveryNEpochs()).fanIn(getFanIn()).build();
  ret.setGradientListeners(gradientListeners.get(index));
  return ret;
}
