{
  while (true) {
    try {
      Pair<WebTarget,Entity> pair=queue.take();
      postReport(pair.getFirst(),pair.getSecond());
    }
 catch (    Exception e) {
      log.error("Exception caught but ignored: " + e.getMessage());
      e.printStackTrace();
    }
 finally {
      try {
        Thread.sleep(100);
      }
 catch (      Exception e) {
        ;
      }
    }
  }
}
