{
  if (k != null)   params.put(PARAM_K,String.valueOf(k));
  if (learningRate != null)   params.put(PARAM_LEARNING_RATE,String.valueOf(learningRate));
  if (finetuneLearningRate != null)   params.put(PARAM_FINETUNE_LR,String.valueOf(finetuneLearningRate));
  if (corruptionLevel != null)   params.put(PARAM_CORRUPTION_LEVEL,String.valueOf(corruptionLevel));
  if (finetuneEpochs != null)   params.put(PARAM_FINETUNE_EPOCHS,String.valueOf(finetuneEpochs));
  if (algorithm != null)   params.put(PARAM_ALGORITHM,algorithm);
  if (epochs != null)   params.put(PARAM_EPOCHS,String.valueOf(epochs));
  return new JSONObject(params).toString();
}
