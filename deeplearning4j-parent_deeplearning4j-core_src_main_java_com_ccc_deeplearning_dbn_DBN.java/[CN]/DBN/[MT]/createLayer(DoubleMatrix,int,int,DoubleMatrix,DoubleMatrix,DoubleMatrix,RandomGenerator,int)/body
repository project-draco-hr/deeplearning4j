{
  RBM ret=new RBM.Builder().useRegularization(useRegularization).withMomentum(momentum).numberOfVisible(nVisible).numHidden(nHidden).withWeights(W).withInput(input).withVisibleBias(vBias).withHBias(hBias).withDistribution(dist).withRandom(rng).renderWeights(renderWeightsEveryNEpochs).fanIn(fanIn).build();
  return ret;
}
