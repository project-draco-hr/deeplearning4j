{
  network.getOutputLayer().setLabels(labels);
  if (!network.isForceNumEpochs()) {
    if (network.isShouldBackProp())     network.backProp(lr,iteration);
  }
 else {
    log.info("Training for " + iteration + " iteration");
    if (network.isShouldBackProp())     network.backProp(lr,iteration);
  }
}
