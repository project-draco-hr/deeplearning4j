{
  StringBuffer errors=new StringBuffer();
  String missing=" is missing\n";
  if (!props.containsKey(JAR_PATH))   errors.append("IterativeReduce JAR path [" + JAR_PATH + "]").append(missing);
  if (!props.containsKey(APP_JAR_PATH))   errors.append("Application JAR path [" + APP_JAR_PATH + "]").append(missing);
  if (!props.containsKey(YARN_MEMORY))   errors.append("YARN memory [" + YARN_MEMORY + "]").append(missing);
  if (!props.containsKey(YARN_MASTER))   errors.append("YARN master class [" + YARN_MASTER + "]").append(missing);
  if (!props.containsKey(YARN_WORKER))   errors.append("YARN worker class [" + YARN_WORKER + "]").append(missing);
  if (errors.length() > 0)   throw new IllegalArgumentException(errors.toString());
}
