{
  int nonOneDims=0;
  for (int i=0; i < shape.length; i++)   if (size(i) != 1)   nonOneDims++;
  int[][] ret=new int[2][nonOneDims];
  int count=0;
  for (int i=0; i < shape.length; i++) {
    if (size(i) != 1) {
      ret[0][count]=size(i);
      ret[1][count]=stride[i];
      count++;
    }
  }
  return ret;
}
