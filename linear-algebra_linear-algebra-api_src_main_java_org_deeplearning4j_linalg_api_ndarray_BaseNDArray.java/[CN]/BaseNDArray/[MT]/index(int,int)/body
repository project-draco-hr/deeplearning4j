{
  if (!isMatrix()) {
    if (isColumnVector()) {
      int idx=linearIndex(row);
      return idx;
    }
 else     if (isRowVector()) {
      int idx=linearIndex(column);
      return idx;
    }
 else     throw new IllegalStateException("Unable to getFromOrigin row/column from a non matrix");
  }
  return row * stride[0] + column * stride[1];
}
