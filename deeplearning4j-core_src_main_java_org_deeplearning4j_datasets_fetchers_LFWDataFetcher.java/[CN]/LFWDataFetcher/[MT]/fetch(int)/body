{
  if (!hasMore())   throw new IllegalStateException("Unable to getFromOrigin more; there are no more images");
  List<DataSet> toConvert=new ArrayList<>(numExamples);
  for (int i=0; i < numExamples; i++, cursor++) {
    if (!hasMore())     break;
    toConvert.add(getDataFor(cursor));
  }
  initializeCurrFromList(toConvert);
  setLabelNames(outcomeList);
}
