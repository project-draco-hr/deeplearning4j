{
  Tensor t=new Tensor(rows,cols,slices);
  for (int i=0; i < slices; i++) {
    DoubleMatrix d=new DoubleMatrix(rows,cols);
    for (int j=0; j < d.length; j++) {
      double val=MathUtils.randomDoubleBetween(min,max);
      if (val == 0)       val+=Math.random();
      d.put(j,val);
    }
    t.setSlice(i,d);
  }
  return t;
}
