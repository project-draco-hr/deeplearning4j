{
  DoubleMatrix ret=new DoubleMatrix(rows(),columns());
  int currI=0, currJ=0;
  for (int i=0; i < ret.length; i++) {
    double sum=0;
    for (int slice=0; i < slices; i++)     sum+=get(currI,currJ,slice);
    ret.put(i,sum);
  }
  for (int slice=0; slice < slices(); slice++) {
  }
  return ret;
}
